{
    "name": "rezaandreannn/satusehat",
    "version" : "1.0.0",
    "description": "Laravel package for Satu Sehat API integration",
    "type": "library",
    "authors": [
        {
            "name": "Rezaandreannn",
            "email": "andreanreza042@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rezaandreannn\\SatuSehat\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rezaandreannn\\SatuSehat\\Providers\\SatuSehatServiceProvider"
            ],
            "aliases": {
                "SatuSehat": "Rezaandreannn\\SatuSehat\\Facades\\SatuSehat"
            }
        }
    },
    "prefer-stable": true
}